"    def __call__(self, img, target):\n","        img = self.transforms(img)\n","        if self.train and torch.rand(1) < 0.5:\n","            img = self.flip(img)\n","            target['masks'] = torch.tensor([self.flip(torch.from_numpy(mask)) for mask in target['masks']])\n","        return img, target\n","\n","class CocoSegmentationDataset(CocoDetection):\n","    def __init__(self, root, annFile, train):\n","        super(CocoSegmentationDataset, self).__init__(root, annFile)\n","        self.transforms = MyTransform(train)\n","\n","    def __getitem__(self, idx):\n","        img, targets = super().__getitem__(idx)\n","        img = Image.fromarray(img.numpy().astype(np.uint8), 'RGB')\n","        masks = np.array([self.coco.annToMask(obj) for obj in targets], dtype=np.uint8)\n","        labels = torch.tensor([obj['category_id'] for obj in targets], dtype=torch.int64)\n","        target = {'labels': labels, 'masks': masks}\n","        img, target = self.transforms(img, target)\n","        return img, target\n","\n","# Load the datasets\n","train_dataset = CocoSegmentationDataset('/content/SatSeg-4/train', '/content/SatSeg-4/train/_annotations.coco.json', get_transform(train=True))\n","test_dataset = CocoSegmentationDataset('/content/SatSeg-4/test', '/content/SatSeg-4/test/_annotations.coco.json', get_transform(train=False))\n","\n","train_loader = DataLoader(train_dataset, batch_size=4, shuffle=True, num_workers=0)\n","test_loader = DataLoader(test_dataset, batch_size=1, shuffle=False, num_workers=0)\n","\n","model = maskrcnn_resnet50_fpn(weights=MaskRCNN_ResNet50_FPN_Weights.COCO_V1)\n","num_classes = 11 + 1  # 11 classes + background\n","in_features = model.roi_heads.box_predictor.cls_score.in_features\n","model.roi_heads.box_predictor = FastRCNNPredictor(in_features, num_classes)\n","model.to(device)\n","\n","optimizer = torch.optim.SGD(model.parameters(), lr=0.005, momentum=0.9, weight_decay=0.0005)\n","scheduler = torch.optim.lr_scheduler.StepLR(optimizer, step_size=3, gamma=0.1)\n","\n","num_epochs = 10\n","for epoch in range(num_epochs):\n","    model.train()\n","    for images, targets in train_loader:\n","        images = list(image.to(device) for image in images)\n","        targets = [{k: v.to(device) for k, v in t.items()} for t in targets]\n","\n","        loss_dict = model(images, targets)\n","        losses = sum(loss for loss in loss_dict.values())\n","\n","        optimizer.zero_grad()\n","        losses.backward()\n","        optimizer.step()\n","\n","    print(f'Epoch: {epoch}, Loss: {losses.item()}')\n","    scheduler.step()\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":377},"id":"6SUFcltXBvAn","executionInfo":{"status":"error","timestamp":1715106054396,"user_tz":-360,"elapsed":488,"user":{"displayName":"Rafsan Jany","userId":"08721334090885411304"}},"outputId":"83cc48ff-3b82-4e41-cd46-9397dd351aee"},"execution_count":49,"outputs":[{"output_type":"error","ename":"TypeError","evalue":"MyTransform.__init__() got an unexpected keyword argument 'transform'","traceback":["\u001b[0;31m---------------------------------------------------------------------------\u001b[0m","\u001b[0;31mTypeError\u001b[0m                                 Traceback (most recent call last)","\u001b[0;32m<ipython-input-49-e4b3820e6b1c>\u001b[0m in \u001b[0;36m<cell line: 45>\u001b[0;34m()\u001b[0m\n\u001b[1;32m     43\u001b[0m \u001b[0;34m\u001b[0m\u001b[0m\n\u001b[1;32m     44\u001b[0m \u001b[0;31m# Load the datasets\u001b[0m\u001b[0;34m\u001b[0m\u001b[0;34m\u001b[0m\u001b[0m\n\u001b[0;32m---> 45\u001b[0;31m \u001b[0mtrain_dataset\u001b[0m \u001b[0;34m=\u001b[0m \u001b[0mCocoSegmentationDataset\u001b[0m\u001b[0;34m(\u001b[0m\u001b[0;34m'/content/SatSeg-4/train'\u001b[0m\u001b[0;34m,\u001b[0m \u001b[0;34m'/content/SatSeg-4/train/_annotations.coco.json'\u001b[0m\u001b[0;34m,\u001b[0m \u001b[0mget_transform\u001b[0m\u001b[0;34m(\u001b[0m\u001b[0mtrain\u001b[0m\u001b[0;34m=\u001b[0m\u001b[0;32mTrue\u001b[0m\u001b